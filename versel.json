{
    "version": 2,
    "builds": [
      {
        "src": "dist/travel-app/server/server.mjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/sitemap.xml",
        "dest": "/assets/sitemap.xml",
        "headers": {
          "Content-Type": "application/xml"
        }
      },
      {
        "src": "/robots.txt",
        "dest": "/assets/robots.txt"
      },
      {
        "src": "/assets/(.*)",
        "headers": {
          "Cache-Control": "public, max-age=31536000"
        }
      },
      {
        "src": "/(.*)",
        "dest": "dist/travel-app/server/server.mjs",
        "status": 200
      }
    ]
  }